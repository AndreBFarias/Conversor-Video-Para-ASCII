<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.24"/>

  <object class="GtkAdjustment" id="adj_h_min">
    <property name="lower">0</property><property name="upper">179</property><property name="value">35</property>
    <property name="step-increment">1</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_h_max">
    <property name="lower">0</property><property name="upper">179</property><property name="value">85</property>
    <property name="step-increment">1</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_s_min">
    <property name="lower">0</property><property name="upper">255</property><property name="value">40</property>
    <property name="step-increment">1</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_s_max">
    <property name="lower">0</property><property name="upper">255</property><property name="value">255</property>
    <property name="step-increment">1</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_v_min">
    <property name="lower">0</property><property name="upper">255</property><property name="value">40</property>
    <property name="step-increment">1</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_v_max">
    <property name="lower">0</property><property name="upper">255</property><property name="value">255</property>
    <property name="step-increment">1</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_erode">
    <property name="lower">0</property><property name="upper">10</property><property name="value">2</property>
    <property name="step-increment">1</property><property name="page-increment">1</property>
  </object>
  <object class="GtkAdjustment" id="adj_dilate">
    <property name="lower">0</property><property name="upper">10</property><property name="value">2</property>
    <property name="step-increment">1</property><property name="page-increment">1</property>
  </object>
  <object class="GtkAdjustment" id="adj_width">
    <property name="lower">40</property><property name="upper">400</property><property name="value">80</property>
    <property name="step-increment">10</property><property name="page-increment">20</property>
  </object>
  <object class="GtkAdjustment" id="adj_height">
    <property name="lower">10</property><property name="upper">200</property><property name="value">22</property>
    <property name="step-increment">5</property><property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adj_sobel">
    <property name="lower">0</property><property name="upper">255</property><property name="value">20</property>
    <property name="step-increment">5</property><property name="page-increment">20</property>
  </object>
  <object class="GtkAdjustment" id="adj_sharpen">
    <property name="lower">0</property><property name="upper">2</property><property name="value">0.5</property>
    <property name="step-increment">0.1</property><property name="page-increment">0.2</property>
  </object>
  <object class="GtkAdjustment" id="adj_pixel_size">
    <property name="lower">1</property><property name="upper">10</property><property name="value">2</property>
    <property name="step-increment">1</property><property name="page-increment">1</property>
  </object>
  <object class="GtkAdjustment" id="adj_palette_size">
    <property name="lower">8</property><property name="upper">256</property><property name="value">256</property>
    <property name="step-increment">8</property><property name="page-increment">32</property>
  </object>

  <object class="GtkWindow" id="calibrator_window">
    <property name="can-focus">False</property>
    <property name="title">Extase em 4R73 - Calibrador de Chroma Key</property>
    <signal name="destroy" handler="on_window_destroy"/>
    <signal name="key-press-event" handler="on_key_press"/>
    <child>
      <object class="GtkBox" id="main_box">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <property name="spacing">2</property>
        <property name="margin-start">4</property>
        <property name="margin-end">4</property>
        <property name="margin-top">4</property>
        <property name="margin-bottom">4</property>

        <!-- ROW 1: Video Displays -->
        <child>
          <object class="GtkBox" id="displays_box">
            <property name="visible">True</property>
            <property name="spacing">4</property>
            <property name="homogeneous">True</property>
            <property name="vexpand">True</property>

            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <property name="label-xalign">0.5</property>
                <child>
                  <object class="GtkAspectFrame" id="aspect_original">
                    <property name="visible">True</property>
                    <property name="ratio">1.333</property>
                    <property name="obey-child">False</property>
                    <child><object class="GtkImage" id="image_original"><property name="visible">True</property></object></child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Original </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <property name="label-xalign">0.5</property>
                <child>
                  <object class="GtkAspectFrame" id="aspect_chroma">
                    <property name="visible">True</property>
                    <property name="ratio">1.333</property>
                    <property name="obey-child">False</property>
                    <child><object class="GtkImage" id="image_chroma"><property name="visible">True</property></object></child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Mascara Chroma </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <property name="label-xalign">0.5</property>
                <child>
                  <object class="GtkEventBox" id="event_ascii">
                    <property name="visible">True</property>
                    <property name="can-focus">True</property>
                    <child>
                      <object class="GtkAspectFrame" id="aspect_ascii">
                        <property name="visible">True</property>
                        <property name="ratio">1.333</property>
                        <property name="obey-child">False</property>
                        <child><object class="GtkImage" id="image_ascii"><property name="visible">True</property></object></child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Preview ASCII (duplo-clique = terminal) </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>
          </object>
          <packing><property name="expand">True</property><property name="fill">True</property></packing>
        </child>

        <!-- ROW 2: HSV Controls -->
        <child>
          <object class="GtkFrame">
            <property name="visible">True</property>
            <child>
              <object class="GtkGrid">
                <property name="visible">True</property>
                <property name="row-spacing">1</property>
                <property name="column-spacing">8</property>
                <property name="margin-start">4</property>
                <property name="margin-end">4</property>
                <property name="margin-top">2</property>
                <property name="margin-bottom">2</property>

                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">H Min:</property><property name="xalign">1</property></object><packing><property name="left-attach">0</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkScale" id="scale_h_min"><property name="visible">True</property><property name="adjustment">adj_h_min</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">1</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">S Min:</property><property name="xalign">1</property></object><packing><property name="left-attach">2</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkScale" id="scale_s_min"><property name="visible">True</property><property name="adjustment">adj_s_min</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">3</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">V Min:</property><property name="xalign">1</property></object><packing><property name="left-attach">4</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkScale" id="scale_v_min"><property name="visible">True</property><property name="adjustment">adj_v_min</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">5</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Erode:</property><property name="xalign">1</property></object><packing><property name="left-attach">6</property><property name="top-attach">0</property></packing></child>
                <child><object class="GtkScale" id="scale_erode"><property name="visible">True</property><property name="adjustment">adj_erode</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">7</property><property name="top-attach">0</property></packing></child>

                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">H Max:</property><property name="xalign">1</property></object><packing><property name="left-attach">0</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkScale" id="scale_h_max"><property name="visible">True</property><property name="adjustment">adj_h_max</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">1</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">S Max:</property><property name="xalign">1</property></object><packing><property name="left-attach">2</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkScale" id="scale_s_max"><property name="visible">True</property><property name="adjustment">adj_s_max</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">3</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">V Max:</property><property name="xalign">1</property></object><packing><property name="left-attach">4</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkScale" id="scale_v_max"><property name="visible">True</property><property name="adjustment">adj_v_max</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">5</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Dilate:</property><property name="xalign">1</property></object><packing><property name="left-attach">6</property><property name="top-attach">1</property></packing></child>
                <child><object class="GtkScale" id="scale_dilate"><property name="visible">True</property><property name="adjustment">adj_dilate</property><property name="digits">0</property><property name="value-pos">right</property><property name="hexpand">True</property><signal name="value-changed" handler="on_hsv_changed"/></object><packing><property name="left-attach">7</property><property name="top-attach">1</property></packing></child>
              </object>
            </child>
            <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Controles HSV </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
          </object>
          <packing><property name="expand">False</property><property name="fill">True</property></packing>
        </child>

        <!-- ROW 3: Mode Toggle + Config + Presets + Acoes -->
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="spacing">4</property>

            <!-- Mode Toggle -->
            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="spacing">8</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child>
                      <object class="GtkRadioButton" id="radio_ascii">
                        <property name="visible">True</property>
                        <property name="label">ASCII</property>
                        <property name="active">True</property>
                        <signal name="toggled" handler="on_mode_toggled"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkRadioButton" id="radio_pixelart">
                        <property name="visible">True</property>
                        <property name="label">PixelArt</property>
                        <property name="group">radio_ascii</property>
                        <signal name="toggled" handler="on_mode_toggled"/>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Modo </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <!-- ASCII Options -->
            <child>
              <object class="GtkFrame" id="frame_ascii_options">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox" id="box_ascii_options">
                    <property name="visible">True</property>
                    <property name="spacing">8</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Larg:</property></object></child>
                        <child><object class="GtkSpinButton" id="spin_width"><property name="visible">True</property><property name="adjustment">adj_width</property><property name="width-chars">4</property><signal name="value-changed" handler="on_ascii_config_changed"/></object></child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Alt:</property></object></child>
                        <child><object class="GtkSpinButton" id="spin_height"><property name="visible">True</property><property name="adjustment">adj_height</property><property name="width-chars">3</property><signal name="value-changed" handler="on_ascii_config_changed"/></object></child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Sobel:</property></object></child>
                        <child><object class="GtkScale" id="scale_sobel"><property name="visible">True</property><property name="adjustment">adj_sobel</property><property name="digits">0</property><property name="value-pos">right</property><property name="width-request">60</property><signal name="value-changed" handler="on_ascii_config_changed"/></object></child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Sharp:</property></object></child>
                        <child><object class="GtkScale" id="scale_sharpen"><property name="visible">True</property><property name="adjustment">adj_sharpen</property><property name="digits">1</property><property name="value-pos">right</property><property name="width-request">50</property><signal name="value-changed" handler="on_ascii_config_changed"/></object></child>
                        <child><object class="GtkCheckButton" id="chk_sharpen"><property name="visible">True</property><property name="active">True</property><signal name="toggled" handler="on_ascii_config_changed"/></object></child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Render:</property></object></child>
                        <child>
                          <object class="GtkComboBoxText" id="combo_render_mode">
                            <property name="visible">True</property>
                            <property name="active">0</property>
                            <items><item>User</item><item>Background</item><item>Ambos</item></items>
                            <signal name="changed" handler="on_render_mode_changed"/>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Config </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <!-- Pixel Art Options -->
            <child>
              <object class="GtkFrame" id="frame_pixelart_options">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox" id="box_pixelart_options">
                    <property name="visible">True</property>
                    <property name="spacing">8</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">PxSize:</property></object></child>
                        <child><object class="GtkSpinButton" id="spin_pixel_size"><property name="visible">True</property><property name="adjustment">adj_pixel_size</property><property name="width-chars">2</property><signal name="value-changed" handler="on_pixel_art_changed"/></object></child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Paleta:</property></object></child>
                        <child><object class="GtkSpinButton" id="spin_palette_size"><property name="visible">True</property><property name="adjustment">adj_palette_size</property><property name="width-chars">3</property><signal name="value-changed" handler="on_pixel_art_changed"/></object></child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> PixelArt </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <!-- Presets Chroma -->
            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="spacing">2</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child><object class="GtkButton" id="btn_preset_studio"><property name="label">Studio</property><property name="visible">True</property><signal name="clicked" handler="on_preset_studio_clicked"/></object></child>
                    <child><object class="GtkButton" id="btn_preset_natural"><property name="label">Natural</property><property name="visible">True</property><signal name="clicked" handler="on_preset_natural_clicked"/></object></child>
                    <child><object class="GtkButton" id="btn_preset_bright"><property name="label">Bright</property><property name="visible">True</property><signal name="clicked" handler="on_preset_bright_clicked"/></object></child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Chroma </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <!-- Presets Resolucao e Luminancia -->
            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="spacing">4</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Res:</property></object></child>
                        <child>
                          <object class="GtkComboBoxText" id="combo_resolution">
                            <property name="visible">True</property>
                            <items><item>Mobile</item><item>Low</item><item>Medium</item><item>High</item><item>VeryHigh</item></items>
                            <signal name="changed" handler="on_resolution_changed"/>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                        <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Lum:</property></object></child>
                        <child>
                          <object class="GtkComboBoxText" id="combo_luminance">
                            <property name="visible">True</property>
                            <items><item>Default</item><item>Blocks</item><item>Simple</item><item>Dense</item><item>Dots</item></items>
                            <signal name="changed" handler="on_luminance_changed"/>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Presets </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <!-- Acoes -->
            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="spacing">2</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child><object class="GtkButton" id="btn_auto_detect"><property name="label">Auto</property><property name="visible">True</property><property name="tooltip-text">[A]</property><signal name="clicked" handler="on_auto_detect_clicked"/></object></child>
                    <child><object class="GtkButton" id="btn_reset"><property name="label">Reset</property><property name="visible">True</property><property name="tooltip-text">[R]</property><signal name="clicked" handler="on_reset_clicked"/></object></child>
                    <child>
                      <object class="GtkButton" id="btn_save_config">
                        <property name="visible">True</property>
                        <property name="tooltip-text">[S]</property>
                        <signal name="clicked" handler="on_save_config_clicked"/>
                        <style><class name="suggested-action"/></style>
                        <child>
                          <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                            <child><object class="GtkImage"><property name="visible">True</property><property name="icon-name">document-save-symbolic</property></object></child>
                            <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Salvar</property></object></child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Acoes </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>
          </object>
          <packing><property name="expand">False</property><property name="fill">True</property></packing>
        </child>

        <!-- ROW 4: Gravacao + Status -->
        <child>
          <object class="GtkBox">
            <property name="visible">True</property>
            <property name="spacing">4</property>

            <child>
              <object class="GtkFrame">
                <property name="visible">True</property>
                <child>
                  <object class="GtkBox">
                    <property name="visible">True</property>
                    <property name="spacing">4</property>
                    <property name="margin-start">4</property>
                    <property name="margin-end">4</property>
                    <property name="margin-top">2</property>
                    <property name="margin-bottom">2</property>
                    <child>
                      <object class="GtkToggleButton" id="btn_record_mp4">
                        <property name="visible">True</property>
                        <property name="tooltip-text">Gravar MP4</property>
                        <signal name="toggled" handler="on_record_mp4_toggled"/>
                        <child>
                          <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                            <child><object class="GtkImage"><property name="visible">True</property><property name="icon-name">media-record-symbolic</property></object></child>
                            <child><object class="GtkLabel"><property name="visible">True</property><property name="label">MP4</property></object></child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkToggleButton" id="btn_record_ascii">
                        <property name="visible">True</property>
                        <property name="tooltip-text">Gravar ASCII</property>
                        <signal name="toggled" handler="on_record_ascii_toggled"/>
                        <child>
                          <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                            <child><object class="GtkImage"><property name="visible">True</property><property name="icon-name">document-save-symbolic</property></object></child>
                            <child><object class="GtkLabel"><property name="visible">True</property><property name="label">ASCII</property></object></child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="btn_preview_terminal">
                        <property name="visible">True</property>
                        <property name="tooltip-text">Preview terminal [T]</property>
                        <signal name="clicked" handler="on_preview_terminal_clicked"/>
                        <child>
                          <object class="GtkBox"><property name="visible">True</property><property name="spacing">2</property>
                            <child><object class="GtkImage"><property name="visible">True</property><property name="icon-name">utilities-terminal-symbolic</property></object></child>
                            <child><object class="GtkLabel"><property name="visible">True</property><property name="label">Terminal</property></object></child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="label"><object class="GtkLabel"><property name="visible">True</property><property name="label"> Gravacao </property><attributes><attribute name="weight" value="bold"/></attributes></object></child>
              </object>
            </child>

            <child>
              <object class="GtkLabel" id="lbl_status">
                <property name="visible">True</property>
                <property name="label">Status: Pronto | Q/Esc para sair</property>
                <property name="hexpand">True</property>
                <property name="xalign">0</property>
                <property name="margin-start">8</property>
              </object>
              <packing><property name="expand">True</property><property name="fill">True</property></packing>
            </child>

          </object>
          <packing><property name="expand">False</property><property name="fill">True</property></packing>
        </child>

      </object>
    </child>
  </object>
</interface>
